<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="appsettings.json" />
    <None Remove="jeebsconfig-secrets.json" />
    <None Remove="jeebsconfig.json" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="appsettings.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="jeebsconfig-secrets.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="jeebsconfig.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\src\Jeebs.Apps.ConsoleApps\Jeebs.Apps.ConsoleApps.csproj" />
    <ProjectReference Include="..\..\src\Jeebs.Services\Jeebs.Services.csproj" />
    <ProjectReference Include="..\..\src\Jeebs.WordPress.Old\Jeebs.WordPress.Old.csproj" />
  </ItemGroup>
  <ProjectExtensions><VisualStudio><UserProperties jeebsconfig_1json__JsonSchema="..\..\src\Jeebs.Config\schema.json" /></VisualStudio></ProjectExtensions>
</Project>
