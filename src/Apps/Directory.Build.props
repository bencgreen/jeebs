<?xml version="1.0" encoding="utf-8"?>
<Project>
	<PropertyGroup>
		<TargetFramework>net5.0</TargetFramework>
		<LangVersion>latest</LangVersion>
		<Nullable>enable</Nullable>
	</PropertyGroup>
	<PropertyGroup>
		<RulesetFile>Apps.ruleset</RulesetFile>
		<RulesetPath>$([MSBuild]::GetPathOfFileAbove('$(RulesetFile)', '$(MSBuildThisFileDirectory)../'))</RulesetPath>
	</PropertyGroup>
	<PropertyGroup>
		<CodeAnalysisAdditionalOptions>$(RulesetPath)</CodeAnalysisAdditionalOptions>
	</PropertyGroup>
</Project>